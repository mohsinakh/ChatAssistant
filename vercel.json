{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxDuration": 60
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "PYTHON_VERSION": "3.9"
    },
    "hooks": {
      "build": "sh vercel-build.sh"
    }
  }
  
